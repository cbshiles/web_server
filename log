function flip(str){
	 var len = str.length
	 for (var i=0; i<len/2; i++){
	     // var tmp = str.get(i)
	 }
	 return str
}

function refine(){
    var msg = '<div class="msg">'+$('#msg').val()+'</div>'
    var brdr = '<br>~~~~~~~~~~~~~<br>'
    return flip(msg)+flip(brdr)
}

$(document).on("click", "#transmit", function () {
    $.ajax ({
      type : 'POST', async : true,
      error : errFunc, url : "post",
      data : refine(), success : connectSuccess
    })
})

